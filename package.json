{
  "name": "fluentui-tailwindcss",
  "version": "0.1.0",
  "description": "Fluent UI design tokens as Tailwind CSS v4 presets",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "fluentui",
    "tailwindcss",
    "design-tokens",
    "css",
    "preset"
  ],
  "files": [
    "dist",
    "presets",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset.css": "./presets/preset.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "tsx scripts/generate-presets.ts",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@fluentui/tokens": "1.0.0-alpha.22",
    "@types/node": "^24.7.2",
    "bumpp": "^10.3.1",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@fluentui/tokens": ">=1.0.0-alpha.22"
  },
  "peerDependenciesMeta": {
    "@fluentui/tokens": {
      "optional": true
    }
  }
}
